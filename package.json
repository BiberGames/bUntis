{
  "name": "buntis",
  "productName": "benjasUntis",
  "version": "0.0.1",
  "description": "untis but good not now",
  "main": "./code/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-web": "echo no."
  },
  "author": "benjamin",
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.4.0",
    "@electron-forge/maker-deb": "^6.4.0",
    "@electron-forge/maker-rpm": "^6.4.0",
    "@electron-forge/maker-squirrel": "^6.4.0",
    "@electron-forge/maker-zip": "^6.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.0",
    "@electron/rebuild": "^3.2.13",
    "electron": "^26.6.7"
  },
  "dependencies": {
    "authenticator": "^1.1.5",
    "buffer": "^6.0.3",
    "electon": "^0.0.1-security",
    "electron-squirrel-startup": "^1.0.0",
    "keytar": "^7.9.0",
    "secure-electron-store": "^4.0.2",
    "webuntis": "^2.0.3"
  },
  "build": {
    "appID": "com.benjagames.buntis"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "bUntis",
            "exe": "buntis.exe"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
